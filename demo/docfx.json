{
  "build": {
    "content": [
      {
        "files": [
          "api/**.md",
          "api/**.yml",
          "api/**/toc.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "index.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.yml"
        ]
      }
    ],
    "globalMetadata": {
      "_appName": "ACME",
      "_appTitle": "ACME Documentation",
      "_appFooter": "ACME Inc.<br/>Generated by <b>DocFX</b>",
      "_enableDiagrams": true,
      "_enableSearch": true,
      "_disableAffix": false,
      "_disableFooter": false,
      "_disableSideFilter": true,
      "_gitContribute": {
        "branch": "master"
      },
      "_gitUrlPattern": "github"
    },
    "template": [
      "default",
      "../discordfx"
    ],
    "markdownEngineName": "markdig",
    "dest": "_site"
  }
}